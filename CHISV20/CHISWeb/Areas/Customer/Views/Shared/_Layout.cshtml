    @RenderSection("scriptspro", required: false)
    <script src="~/Customer/js/pages/@(ViewContext.RouteData.Values["controller"].ToString())-@(ViewContext.RouteData.Values["Action"].ToString()).js" ></script>
    @RenderSection("scripts", required: false)  
    <script>
        //载入最后复写样式
        $(function () {
            $("head").append('<link rel="stylesheet" href="/Customer/css/common-overide-last.css" />');
        });
    </script>
